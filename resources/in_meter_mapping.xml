<?xml version='1.0' encoding='UTF-8' ?>
<beanio xmlns="http://www.beanio.org/2012/03" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">

    <stream name="meters" format="csv">
        <parser>
            <property name="delimiter" value="," />
        </parser>
        <record name="detail" minOccurs="0" maxOccurs="unbounded" class="com.ouc.model.Meter">
            <field name="meterBadge"/>
            <field name="manufactureCode" minOccurs="0"/>
            <field name="modelCode" minOccurs="0"/>
            <field name="meterType" minOccurs="0"/>
            <field name="servicePointID" minOccurs="0"/>
            <field name="servicePointType" minOccurs="0"/>
            <field name="premiseID" minOccurs="0"/>
            <field name="premiseType" minOccurs="0"/>
        </record>
    </stream>
</beanio>